<h1>Hi <%= users %></h1>
<form action="/approval" method="POST">
    
    <table>
        <% for(var i=0; i < users.length; i++) { %>
            <tr>
                <td><%= users[i].userId %></td>
                <td><%= users[i].displayName %></td>
                <td><%= users[i].email %></td>
                <td><button type="submit" name="aproveid" value='<%= users[i].userid %>'>Approve</button></td>
                <td><button type="submit" name="rejectid" value='<%= users[i].userid %>'>Reject</button></td>
            </tr>
        <% } %>
    </table>
</form>